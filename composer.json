{
    "name": "maximilliangroup/casanova",
    "description": "Adds a video background to a specific Gutenberg group block.",
    "type": "wordpress-plugin",
    "require": {
        "php": "^7.2",
  
        "humanmade/s3-uploads": "^3.0",
        "wp-cli/wp-cli": "^2.10",
        "johnpbloch/wordpress": "^6.5",
        "roots/wordpress": "^6.5"
  
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
          "casanova\\": "casanova/"
        },
        "exclude-from-classmap": [
          "/wordpress/",
          "/wp-content/"
      ]
    },
    "authors": [
        {
            "name": "MaximillianGroup (Max Barrett)",
            "email": "maximilliangroup@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true,
        "composer/installers": true,
        "roots/wordpress-core-installer": true,
        "mouf/nodejs-installer": true
      },
        "platform": {
          "php": "7.2"
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.1"
  
    }
  }